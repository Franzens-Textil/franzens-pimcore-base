{
    "name": "pimcore/skeleton",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4",
            "ext-exif": "1",
            "ext-gd": "1",
            "ext-intl": "1",
            "ext-mysqli": "1",
            "ext-pdo_mysql": "1",
            "ext-zend-opcache": "1",
            "ext-zip": "1"
        }
    },
    "require": {
        "php": ">=7.4",
        "pimcore/customer-management-framework-bundle": "2.6.2",
        "pimcore/pimcore": "6.9.5",
        "aws/aws-sdk-php": "3.171.0",
        "alpin11/pimcore-s3": "dev-master"
    },
    "require-dev": {
        "cache/integration-tests": "^0.16.0",
        "codeception/codeception": "~2.4.5",
        "symfony/phpunit-bridge": "*"
    },
    "autoload": {
        "psr-4": {
            "": ["src/"],
            "Pimcore\\Model\\DataObject\\": "var/classes/DataObject",
            "Pimcore\\Model\\Object\\": "var/classes/Object",
            "Website\\": "legacy/website/lib"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/alpin11/pimcore-s3.git"
        }
    ]
}