{
    "name": "pimcore/skeleton",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "process-timeout": 0,
        "platform": {
            "php": "8.0",
            "ext-exif": "1",
            "ext-gd": "1",
            "ext-intl": "1",
            "ext-mysqli": "1",
            "ext-pdo_mysql": "1",
            "ext-zend-opcache": "1",
            "ext-zip": "1"
        }
    },
    "require": {
        "league/flysystem-aws-s3-v3": "^2.1",
        "php-http/guzzle7-adapter": "^0.1.1",
        "pimcore/customer-management-framework-bundle": "3.2.2",
        "pimcore/pimcore": "10.1.3"
    },
    "require-dev": {
        "cache/integration-tests": "^0.17.0",
        "codeception/codeception": "^4.1.12",
        "codeception/module-symfony": "^1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "suggest": {
        "pimcore/data-hub": "Universal data interface for GraphQL, CSV and other formats"
    },
    "autoload": {
        "psr-4": {
          "App\\": "src/",
          "Pimcore\\Model\\DataObject\\": "var/classes/DataObject"
        }
    }
}
